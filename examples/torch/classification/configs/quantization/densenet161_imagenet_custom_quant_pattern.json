{
    "model": "densenet161",
    "pretrained": true,

    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size": 32,
    "epochs": 1,
    "multiprocessing_distributed": true,
    "optimizer": {
        "type": "Adam",
        "base_lr": 1e-6
    },
    "compression": {
        "algorithm": "quantization",
        "quantize_patterns": [["1 cat", "2 batch_norm", "3 cat", "1 -> 2", "1 -> 3", "2 -> 3"]],
        "initializer": {
            "range": {
                "num_init_samples": 320
            }
        }
    }
}